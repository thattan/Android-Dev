<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.listmaker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/pizza_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/pizza_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ListMaker">
        <activity android:name=".ListInstanceActivity"></activity>
        <activity android:name=".ListsActivity" />
        <activity android:name=".CreateListItemActivity" />
        <activity android:name=".CreateListActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>